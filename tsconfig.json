{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/Components/*"],
    },
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "lib": ["dom", "es6", "esnext.asynciterable"],
    "jsx": "react",
    "downlevelIteration": true,
    "esModuleInterop": true
  },
  "include": ["./src/**/*"],
  "files": ["./globals.d.ts"],
  "exclude": ["node_modules", ".npm"]
}